@model  int
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<td style="width:150px">
    <div class="btn-group" role="group">
        <a class="btn btn-sm btn-success" asp-action="Details" asp-route-id="@Model" title="Details">
            <i class="far fa-list-alt"></i>
        </a>
        <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@Model" title="Edit">
            <i class="far fa-edit"></i>
        </a>
        <a class="btn btn-sm btn-danger" asp-action="Delete" asp-route-id="@Model" title="Delete">
            <i class="far fa-trash-alt"></i>
        </a>
    </div>
</td>

